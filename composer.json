{
	"name": "oops/slim-nette-bridge",
	"description": "Slim Framework bridge for Nette DI.",
	"keywords": ["nette", "di", "slim"],
	"type": "library",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Jiří Pudil",
			"email": "me@jiripudil.cz",
			"homepage": "https://jiripudil.cz"
		}
	],
	"support": {
		"email": "me@jiripudil.cz",
		"issues": "https://github.com/o2ps/SlimNetteBridge/issues"
	},
	"require": {
		"php": ">= 7.1.0",
		"nette/di": "^2.4.0",
		"nette/utils": "^2.4.0",
		"psr/container": "1.0.0",
		"slim/slim": "^3.0.0"
	},
	"require-dev": {
		"jakub-onderka/php-parallel-lint": "^0.9.0",
		"nette/bootstrap": "^2.4.0",
		"nette/tester": "^1.7.0",
		"phpstan/phpstan": "^0.8.0"
	},
	"autoload": {
		"psr-4": {
			"Oops\\SlimNetteBridge\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"OopsTests\\SlimNetteBridge\\": "tests/"
		}
	}
}
